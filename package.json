{
  "name": "apps-diya-client",
  "version": "1.2.19",
  "private": true,
  "scripts": {
    "start": "parcel index.html --open",
    "server": "cross-env PORT=9999 ts-node --project tsconfig.tsnode.json src/db.ts",
    "build": "parcel build index.html",
    "test": "cross-env NODE_ENV='test' jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "elint": "eslint './src/**/*.{js,ts,tsx}'",
    "elint:fix": "yarn elint --quiet --fix",
    "tlint": "yarn tslint -p . -c tslint.json 'src/**/*.ts'",
    "tlint:fix": "yarn lint --fix",
    "lint": "yarn elint && yarn tlint",
    "lint:fix": "yarn elint:fix && yarn tlint:fix",
    "types": "tsc --noEmit -p .",
    "types:watch:verbose": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling yarn types --watch --preserveWatchOutput",
    "types:watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling yarn types --watch",
    "validate:noFix": "yarn lint && yarn types && yarn test",
    "validate": "yarn lint:fix && yarn types && yarn test"
  },
  "dependencies": {
    "@date-io/date-fns": "1.3.11",
    "@material-ui/core": "4.7.1",
    "@material-ui/pickers": "3.2.8",
    "@react-google-maps/api": "1.7.6",
     "bulma": "0.8.0",
    "date-fns": "2.0.0-beta.5",
    "formik": "1.5.8",
    "match-sorter": "4.0.2",
    "react": "16.10.0",
    "react-autosuggest": "9.4.3",
    "react-d3-speedometer": "0.7.3",
    "react-dom": "16.10.0",
    "react-table": "7.0.0-beta.8",
    "react-toastify": "5.4.1",
     "technoidentity-devfractal": "nandigamchandu/devfractal#1.2.19v",
    "technoidentity-utils": "nandigamchandu/utils#1.2.19v",
    "victory": "^33.1.3",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "4.1.0",
    "@testing-library/react": "9.2.0",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.8",
    "@types/react": "16.9.3",
    "@types/react-autosuggest": "9.3.11",
      "@types/react-dom": "16.9.1",
    "@types/yup": "0.26.24",
    "@types/googlemaps": "3.37.7",
     "@types/match-sorter": "2.3.0",
      "@types/victory": "33.0.0",
    "@typescript-eslint/eslint-plugin": "2.7.0",
    "@typescript-eslint/parser": "2.7.0",
    "awesome-typescript-loader": "^5.2.1",
    "bulmaswatch": "^0.7.5",
    "cross-env": "6.0.0",
    "eslint": "6.4.0",
    "jest": "24.9.0",
    "node-sass": "4.12.0",
    "parcel-bundler": "1.12.3",
    "prettier": "1.18.2",
    "react-datepicker": "2.9.6",
    "rimraf": "3.0.0",
    "technoidentity-dev-utils": "nandigamchandu/dev-utils#1.2.19v",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-immutable": "6.0.1",
    "tslint-react": "4.1.0",
    "typescript": "3.6.3",
    "typescript-tslint-plugin": "0.5.4"
  },
  "proxy": "http://localhost:9999",
  "resolutions": {
    "react": "16.10.0",
    "react-dom": "16.10.0"
